{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-openhack-MDW"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/CopyPipeline_ADX')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Copy_3wm",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [
							{
								"name": "Source",
								"value": "inbound-processes//"
							},
							{
								"name": "Destination",
								"value": "Newblobtable"
							}
						],
						"typeProperties": {
							"source": {
								"type": "JsonSource",
								"storeSettings": {
									"type": "AzureBlobFSReadSettings",
									"recursive": true,
									"wildcardFileName": "*"
								}
							},
							"sink": {
								"type": "AzureDataExplorerSink",
								"ingestionMappingName": "ExelonInput_mapping"
							},
							"enableStaging": false,
							"translator": {
								"type": "TabularTranslator",
								"mappings": [
									{
										"source": {
											"path": "$['quote_id']"
										},
										"sink": {
											"name": "quote_id",
											"type": "String"
										}
									},
									{
										"source": {
											"path": "$['policy_id']"
										},
										"sink": {
											"name": "policy_id",
											"type": "String"
										}
									},
									{
										"source": {
											"path": "$['tel_1']"
										},
										"sink": {
											"name": "tel_1",
											"type": "String"
										}
									},
									{
										"source": {
											"path": "$['tel_2']"
										},
										"sink": {
											"name": "tel_2",
											"type": "String"
										}
									},
									{
										"source": {
											"path": "$['email_1']"
										},
										"sink": {
											"name": "email_1",
											"type": "String"
										}
									},
									{
										"source": {
											"path": "$['geo_street_1']"
										},
										"sink": {
											"name": "geo_street_1",
											"type": "String"
										}
									},
									{
										"source": {
											"path": "$['geo_street_2']"
										},
										"sink": {
											"name": "geo_street_2",
											"type": "String"
										}
									},
									{
										"source": {
											"path": "$['geo_street_3']"
										},
										"sink": {
											"name": "geo_street_3",
											"type": "String"
										}
									},
									{
										"source": {
											"path": "$['geo_town']"
										},
										"sink": {
											"name": "geo_town",
											"type": "String"
										}
									},
									{
										"source": {
											"path": "$['geo_postcode']"
										},
										"sink": {
											"name": "geo_postcode",
											"type": "String"
										}
									},
									{
										"source": {
											"path": "$['geo_country_code']"
										},
										"sink": {
											"name": "geo_country_code",
											"type": "String"
										}
									},
									{
										"source": {
											"path": "$['quote_value']"
										},
										"sink": {
											"name": "quote_value",
											"type": "String"
										}
									},
									{
										"source": {
											"path": "$['quote_currency']"
										},
										"sink": {
											"name": "quote_currency",
											"type": "String"
										}
									},
									{
										"source": {
											"path": "$['quote_given_date']"
										},
										"sink": {
											"name": "quote_given_date",
											"type": "String"
										}
									},
									{
										"source": {
											"path": "$['quote_valid_date']"
										},
										"sink": {
											"name": "quote_valid_date",
											"type": "String"
										}
									}
								]
							}
						},
						"inputs": [
							{
								"referenceName": "SourceDataset_3wm",
								"type": "DatasetReference"
							}
						],
						"outputs": [
							{
								"referenceName": "DestinationDataset_3wm",
								"type": "DatasetReference"
							}
						]
					}
				],
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/DestinationDataset_3wm')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/DestinationDataset_3wm')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "AzureDataExplorer1",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "AzureDataExplorerTable",
				"schema": [],
				"typeProperties": {
					"table": "ExelonInput"
				}
			},
			"dependsOn": []
		}
	]
}